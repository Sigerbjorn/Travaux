<script>
// Script pour remplacer l'image par défaut d'un cours par l'image perso de l'enseignant
// pré-requis #1 = activer l'affichage du résumé qlq soit le nombre de cours : Administration du site > Présentation > Cours : Limite de cours avec résumé = 10000
// pré-requis #2 = limiter les fichiers de résumé de cours à 1 et aux formats img (jpg/png/gif) : Administration du site > Présentation > Cours : Limite du nombre de fichiers de résumé des cours = 1 | Extensions des fichiers de résumé des cours = .jpg,.gif,.png
window.onload=function(){
	if (document.getElementsByClassName("courses") != undefined){

		var contenuBoitesCours = document.getElementsByClassName("coursebox");
		for(var i=0; i<contenuBoitesCours.length;i++){
        	var imgDuCours = contenuBoitesCours[i].getElementsByClassName("content")[0].getElementsByClassName("courseimage")[0];
          	if(imgDuCours!= undefined){
				contenuBoitesCours[i].appendChild(imgDuCours);
				var srcImg = imgDuCours.getElementsByTagName("img")[0].src;
				imgDuCours.style.background = 'url('+srcImg+')';
			}else{
				var imgCorporate = document.createElement("img");
				imgCorporate.setAttribute("src","https://moodle.univ-valenciennes.fr/pluginfile.php/48/mod_resource/content/6/cours.jpg");
				contenuBoitesCours[i].appendChild(imgCorporate);
			}
		}	
	}
// fin Window onload
}

//Script complémentaire au script pour remplacer l'image par défaut d'un cours par l'image perso de l'enseignant
//Toggle pour l'affichage du résumé (ré-activer pour pvr afficher l'image du cours)
if (document.getElementsByClassName("courses")[0] != undefined){
var contenuBoitesCours = document.getElementsByClassName("coursebox");
for(var i=0; i<contenuBoitesCours.length;i++){
	if(contenuBoitesCours[i].getElementsByClassName("summary")[0] != undefined){
		contenuBoitesCours[i].getElementsByClassName("summary")[0].style.display="none";
		var infoResumeCours = document.getElementsByClassName("moreinfo");
		infoResumeCours[i].innerHTML = "<img class=\"icon \" alt=\"Résumé\" title=\"Résumé\" src=\"https://moodle.univ-valenciennes.fr/theme/image.php/adaptable/core/1509442261/i/info\">";
	}
}
document.getElementsByClassName('courses')[0].addEventListener('click', function(e){
	var initElem = e.target;
	if(initElem.className.trim() == "icon" && initElem.getAttribute("title") == "Résumé"){
	    var courseBox = closest(initElem, ".coursebox", ".courses");
	    var summaryElem = courseBox.getElementsByClassName("summary");
	    summaryElem = summaryElem[0];
	    if(summaryElem.style.display=="none"){
	        summaryElem.style.display="block";
	    }else{
	        summaryElem.style.display="none";
	    }
	 }
});

function closest(el, selector, stopSelector) {
    var retval = null;
		while (el) {
			if (el.matches(selector)) {
	    		retval = el;
	    		break
			} else if (stopSelector && el.matches(stopSelector)) {
	    		break
			}
			el = el.parentElement;
 		}
	return retval;
}
}

//Ajout de l'info et du lien vers freepik au dessus du champ "Fichiers de résumé des cours" des paramètres d'un espace de cours

if(document.getElementsByTagName('form')[0].action=="https://moodle.univ-valenciennes.fr/course/edit.php"){
var personnalisationImgCours = document.createElement("div");
personnalisationImgCours.setAttribute("id","personnalisationImgCours");
personnalisationImgCours.innerHTML = "Personnalisez l'image de votre cours avec une illustration gratuite et libre issue de ";

var lienFreepik = document.createElement("a");
lienFreepik.setAttribute("href","https://fr.freepik.com/");
lienFreepik.setAttribute("target","_blank");
lienFreepik.setAttribute("alt","https://fr.freepik.com/");
lienFreepik.setAttribute("title", "https://fr.freepik.com/");
lienFreepik.textContent = 'https://fr.freepik.com/';

personnalisationImgCours.appendChild(lienFreepik);
	document.getElementById("fitem_id_overviewfiles_filemanager").parentNode.insertBefore(personnalisationImgCours, document.getElementById("fitem_id_overviewfiles_filemanager"));
}
</script></body>
